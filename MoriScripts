repeat
    task.wait()
until game.GameId ~= nil
repeat
    task.wait()
until game:IsLoaded()

if getgenv().loaded then
    return
end
getgenv().loaded = true

universeid = game.GameId

--ps99

if universeid == 8737899170 or universeid == 15502339080 or universeid == 15588442388 or universeid == 17503543197 or universeid == 16498369169 then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ImDigitalz/MoriScripts/main/PS99"))()
end
